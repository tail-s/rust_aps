use std::io;

fn main() {
    let mut input = String::new();
    
    io::stdin().read_line(&mut input).unwrap();
    
    let input: Vec<f64> = input
        .trim()
        .split_whitespace()
        .map(|x| x.parse().expect("err"))
        .collect();
    
    print!("{}", input[0] / input[1]);
}