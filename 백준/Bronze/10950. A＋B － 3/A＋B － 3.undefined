fn main() {
    let stdin = std::io::stdin();
    let mut t = String::new();
    stdin.read_line(&mut t).unwrap();
    let t: u32 = t.trim().parse().unwrap();
    
    for i in 1..=t {
        let mut buf = String::new();
        stdin.read_line(&mut buf).unwrap();
        let buf: Vec<u32> = buf
            .trim()
            .split_whitespace()
            .map(|x| x.parse().expect("err"))
            .collect();
        println!("{}", buf[0] + buf[1]);
    }
    
}