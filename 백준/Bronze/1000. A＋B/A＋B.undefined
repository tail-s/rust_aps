use std::io;

fn main() {
    let mut input = String::new();
    
    io::stdin().read_line(&mut input).unwrap();
    
    let nums: Vec<u32> = input
        .trim()
        .split_whitespace()
        .map(|x| x.parse().expect("Err2"))
        .collect();
    print!("{}", nums[0] + nums[1]);
}